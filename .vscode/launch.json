  "version": "0.2.0",
  "configurations": [
    {
      "name": "All Tests Windows",
      "python": "${workspaceFolder}/venv/Scripts/python",
      "type": "python",
      "request": "launch",
      "module": "unittest",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "DEVELOPMENT": "TRUE"
      }
    },
    {
      "name": "tiempo_es Windows",
      "python": "${workspaceFolder}/venv/Scripts/python",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/tests/eltiempo_test.py",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "All Tests MacOS",
      "python": "${workspaceFolder}/venv/bin/python",
      "type": "python",
      "request": "launch",
      "module": "unittest",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "DEVELOPMENT": "TRUE"
      }
    },
    {
      "name": "tiempo_es MacOs",
      "python": "${workspaceFolder}/venv/bin/python",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/tests/eltiempo_test.py",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
